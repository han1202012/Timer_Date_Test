<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="shuliang.han.time_date_test"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="shuliang.han.time_date_test.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="shuliang.han.time_date_test.ClockActivity"></activity>
        <activity android:name="shuliang.han.time_date_test.ChronometerActivity"></activity>
        <activity android:name="shuliang.han.time_date_test.CalendarActivity"></activity>
        <activity android:name="shuliang.han.time_date_test.TimePickerActivity"></activity>
        <activity android:name="shuliang.han.time_date_test.DatePickerActivity"></activity>
        
    </application>

</manifest>
